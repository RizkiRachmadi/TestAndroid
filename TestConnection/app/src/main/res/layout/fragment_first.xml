<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstFragment">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Detection System"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/top_horizontal" />


    <TextView
        android:id="@+id/tv_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 1"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/start_vertical"
        app:layout_constraintTop_toBottomOf="@id/top_horizontal_1" />

    <EditText
        android:id="@+id/et_1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_1"
        app:layout_constraintEnd_toEndOf="@id/center_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_2"
        android:layout_marginTop="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 2"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/start_vertical"
        app:layout_constraintTop_toBottomOf="@id/tv_1" />

    <EditText
        android:id="@+id/et_2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_2"
        app:layout_constraintEnd_toEndOf="@id/center_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_3"
        android:layout_marginTop="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 3"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/start_vertical"
        app:layout_constraintTop_toBottomOf="@id/tv_2" />

    <EditText
        android:id="@+id/et_3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_3"
        app:layout_constraintEnd_toEndOf="@id/center_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_4"
        android:layout_marginTop="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 4"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/start_vertical"
        app:layout_constraintTop_toBottomOf="@id/tv_3" />

    <EditText
        android:id="@+id/et_4"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_4"
        app:layout_constraintEnd_toEndOf="@id/center_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 5"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/center_vertical_1"
        app:layout_constraintTop_toBottomOf="@id/top_horizontal_1" />

    <EditText
        android:id="@+id/et_5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_5"
        app:layout_constraintEnd_toEndOf="@id/end_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_6"
        android:layout_marginTop="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 6"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/center_vertical_1"
        app:layout_constraintTop_toBottomOf="@id/tv_5" />

    <EditText
        android:id="@+id/et_6"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_6"
        app:layout_constraintEnd_toEndOf="@id/end_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_7"
        android:layout_marginTop="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 7"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/center_vertical_1"
        app:layout_constraintTop_toBottomOf="@id/tv_6" />

    <EditText
        android:id="@+id/et_7"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_7"
        app:layout_constraintEnd_toEndOf="@id/end_vertical"
        android:text="0" />

    <TextView
        android:id="@+id/tv_8"
        android:layout_marginTop="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Label 8"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@id/center_vertical_1"
        app:layout_constraintTop_toBottomOf="@id/tv_7" />

    <EditText
        android:id="@+id/et_8"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_8"
        app:layout_constraintEnd_toEndOf="@id/end_vertical"
        android:text="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.48" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/center_vertical_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.52" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/end_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_horizontal_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />


</androidx.constraintlayout.widget.ConstraintLayout>